# Armbian First Boot Config File
# This file was generated by https://github.com/clueboard/af

# Network Settings
PRESET_NET_CHANGE_DEFAULTS="1"

## Ethernet
PRESET_NET_ETHERNET_ENABLED="1"

## WiFi
#PRESET_NET_WIFI_ENABLED="1"
#PRESET_NET_WIFI_SSID="MySSID"
#PRESET_NET_WIFI_KEY="MyWiFiKEY"
#PRESET_NET_WIFI_COUNTRYCODE="GB"
#PRESET_CONNECT_WIRELESS="n"
## Static IP
PRESET_NET_USE_STATIC="1"
PRESET_NET_STATIC_IP="{{ upstream_ip }}"
PRESET_NET_STATIC_MASK="{{ upstream_netmask }}"
PRESET_NET_STATIC_GATEWAY="{{ upstream_gw }}"
PRESET_NET_STATIC_DNS="{{ ' '.join(upstream_dns) }}"

# System
SET_LANG_BASED_ON_LOCATION="y"
PRESET_LOCALE="en_US.UTF-8"
PRESET_TIMEZONE="Etc/UTC"

# Root
PRESET_ROOT_KEY="ssh-rsa  zwhite@k8s-control"

# User
PRESET_USER_NAME="zwhite"
PRESET_USER_KEY="ssh-rsa  zwhite@k8s-control"
PRESET_DEFAULT_REALNAME="Zach White"
PRESET_USER_SHELL="bash"
